<!doctype html>
<html>

<head>
  <title><%= title %></title>
  <link rel="stylesheet" href="/stylesheets/style.css" />
  <!-- add CSS to style the board! -->
</head>

<body>
  <h1><%= title %></h1>
  <p><a href="/new">Add message</a></p>
  <% messages.forEach(element => { %>
  <article>
    <div>
      <p><%= element.author %></p>
      <p><%= element.added %></p>
    </div>
    <p><%= element.text %></p>
  </article>
  <% }); %>
</body>

</html>